plugin.tx_w3caichatbot_chatbot {
    settings {
        layout = 1
        stream = 1
    }
}

chatbot_ajax = PAGE
chatbot_ajax.typeNum = 2999
chatbot_ajax.config {
    disableAllHeaderCode = 1
    xhtml_cleaning = 0
    admPanel = 0
    debug = 0
    no_cache = 1
}

lib.chatbot = USER
lib.chatbot {
  userFunc = TYPO3\CMS\Extbase\Core\Bootstrap->run
  extensionName = W3cAichatbot
  pluginName = ChatbotAjax
  vendorName = W3code
}

chatbot_ajax.10 < lib.chatbot
